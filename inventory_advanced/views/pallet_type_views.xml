<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_pallet_type_tree" model="ir.ui.view">
        <field name="name">pallet.type.tree</field>
        <field name="model">pallet.type</field>
        <field name="arch" type="xml">
            <list string="Pallet Types" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>


    <record id="action_pallet_type" model="ir.actions.act_window">
        <field name="name">Pallet Types</field>
        <field name="res_model">pallet.type</field>
        <field name="view_mode">list,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create pallet types to use in inventory configuration.
            </p>
        </field>
    </record>
</odoo>
